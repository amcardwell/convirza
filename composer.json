{
	"name": "skidaatl/convirza",
	"description": "Convirza API wrapper with support for Laravel 5.",
	"license": "MIT",
	"authors": [
		{
			"name": "Adam",
			"email": "adam@trustdale.com"
		}
	],
	"require": {
		"php": ">=7.0.0",
		"guzzlehttp/guzzle": "^6.2"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "^6.5"
	},
	"autoload": {
		"psr-4": {
			"Skidaatl\\Convirza\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Skidaatl\\Convirza\\ConvirzaServiceProvider"
			],
			"aliases": {
				"Convirza": "Skidaatl\\Convirza\\ConvirzaFacade"
			}
		}
	}
}
